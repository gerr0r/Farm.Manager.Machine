  "bugs": {
    "url": "https://github.com/gerr0r/Farm.Manager.Machine/issues"
  },
  "homepage": "https://github.com/gerr0r/Farm.Manager.Machine#readme",
  "dependencies": {
    "@apollo/federation": "^0.25.2",
    "apollo-server": "^2.25.2",
    "graphql": "^15.5.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.6.2",
    "sequelize-cli": "^6.2.0"
  },
  "devDependencies": {
    "dotenv": "^10.0.0",
    "nodemon": "^2.0.7"
  }
