{
  "name": "farm.manager.machine",
  "version": "1.0.0",
  "description": "Farm Manager Machine Microservice",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "predev": "npm run migrate && npm run seed",
    "dev": "nodemon -r dotenv/config",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all",
    "unseed": "npx sequelize-cli db:seed:undo:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gerr0r/Farm.Manager.Machine.git"
  },
